{
  "plugins": [
    "standard"
  ],
  "env": {},
  "extends": ["standard"],
  "rules": {
    "semi": 0,
    "indent": ["error", 4],
    "object-curly-spacing": ["error", "always"],
    "space-before-function-paren": ["error", {"anonymous": "always", "named": "never", "asyncArrow": "always"}]
  }
}
